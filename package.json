    "laravel-mix": "3.0.0",
    "laravel-mix-purgecss": "3.0.0",
    "lodash": "4.17.11",
    "material-design-icons-iconfont": "4.0.3",
    "popper.js": "1.14.6",
    "print-js": "1.0.54",
    "roboto-fontface": "0.10.0",
    "toastr": "2.1.4",
    "vee-validate": "2.1.3",
    "vue": "2.5.17",
    "vue-json-to-csv": "1.1.7",
    "vue-moment": "4.0.0",
